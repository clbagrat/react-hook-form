:{"src/Examples/SelectForm.js":"import React from 'react';\nimport { useForm, useField } from '@monorepo/hooks/Form';\n\nexport const SelectForm = ({ onSubmit, onChange }) => {\n  const [form, handleSubmit] = useForm({ onSubmit, onChange });\n  const [fruit] = useField('fruit', form);\n  const [vegie] = useField('vegie', form, null, []);\n\n  return (\n    <form onSubmit={handleSubmit}>\n      <select {...fruit}>\n        <option value=\"\" disabled>\n          Select a fruit\n        </option>\n        <option value=\"banana\">Banana</option>\n        <option value=\"orange\">Orange</option>\n      </select>\n      <hr />\n      <select multiple {...vegie}>\n        <option value=\"tomato\">Tomato</option>\n        <option value=\"potato\">Potato</option>\n        <option value=\"cucumber\">Cucumber</option>\n      </select>\n      <hr />\n      <button type=\"submit\">Send</button>\n    </form>\n  );\n};\n"},"lineCount":null}},"error":null,"hash":"a50d6a3499a446d134e0f905e93368f4","cacheData":{"env":{}}}